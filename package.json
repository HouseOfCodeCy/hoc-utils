{
  "name": "@houseofcodecy/hoc-utils",
  "version": "2.19.0",
  "description": "A component library consisting of HoC Utilities",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/HouseOfCodeCy/hoc-utils.git"
  },
  "scripts": {
    "build": "tsc",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build-watch": "yarn build:esm --watch && yarn build:cjs --watch",
    "link-dev": "yarn build && yarn link && cp package.json ./lib && yarn run build-watch",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prettier"
  },
  "keywords": [],
  "author": "HoC",
  "license": "MIT",
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-loader": "^8.3.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "axios": "1.2.1"
  }
}
